<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.Pages.DataPage"
             Title=""
             Style="{DynamicResource DarkPageStyle}">
    <VerticalStackLayout>
        <Label 
    Text="User Statistics"
    VerticalOptions="Center" 
    HorizontalOptions="Center"
    TextColor="White"
    FontFamily="Montserrat"
            FontAttributes="Bold"/>
        <Label 
            Text="Last used strength sevel: -"
            x:Name="strengthText"
            VerticalOptions="Center" 
            HorizontalOptions="Center"
            TextColor="White"
            FontFamily="Montserrat"/>
        <Label 
    Text="Last used position: -"
    x:Name="positionText"
    VerticalOptions="Center" 
    HorizontalOptions="Center"
            TextColor="White"
            FontFamily="Montserrat"/>
        <Label 
    Text="Last used time: - minutes, - seconds"
    x:Name="timeText"
    VerticalOptions="Center" 
    HorizontalOptions="Center"
            TextColor="White"
            FontFamily="Montserrat"/>
    </VerticalStackLayout>
</ContentPage>